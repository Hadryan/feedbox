{% extends "base.html.twig" %}

{% block title %}Feedbox - Install{% endblock %}

{% block content %}

<div class="container">
    <h1 class="page-header">
        <img src="img/feedbox128.png" alt="FeedBox"/> {{ title }}
    </h1>

    {% for message in msg %}
    <div class="alert alert-success">{{ message }}</div>
    {% endfor %}

    <div class="alert alert-success">
        Done. Go to <a href="{{ base_url }}">index</a>.
    </div>
</div>

{% endblock %}